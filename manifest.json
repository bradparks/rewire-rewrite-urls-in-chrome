{
    "manifest_version": 2,
    "name": "Rewire",
    "version": "0.1",
    "author": "Alan Shreve (@inconshreveable)",
    "description": "Transform distracting browsing habits into productive time.",
    "background": {
        "page": "background.html"
    },
    "omnibox": { "keyword": "unwire" },
    "options_page": "options.html",
    "permissions": ["contextMenus", "notifications", "tabs", "webRequest", "webRequestBlocking", "*://*/"],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "browser_action": {
        "default_icon": {
            "19": "rewire16.png"
        },
        "default_title": "Rewire"
    },
    "icons": {
        "16": "rewire16.png"
    }
}
